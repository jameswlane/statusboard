{
  "name": "www",
  "version": "1.0.0",
  "private": true,
  "main": "lib/data/index.js",
  "scripts": {
    "dev": "node ./bin/build.mjs --dev",
    "build": "node ./bin/build.mjs --prod",
    "clean": "rm -rf lib/data/daily && rm -rf lib/data/latest.json",
    "test": "tap",
    "lint": "eslint \"**/*.js\"",

  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jameswlane/statusboard/issues"
  },
  "homepage": "https://github.com/jameswlane/statusboard#readme",
  "devDependencies": {
    "@npmcli/eslint-config": "^4.0.1",
    "esbuild": "^0.17.12",
    "http-server": "^14.1.1",
    "tap": "^16.3.4"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "data": "workspace:^1.0.0",

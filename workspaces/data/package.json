  },
  "homepage": "https://github.com/npm/statusboard#readme",
  "dependencies": {
    "@octokit/graphql": "^5.0.5",
    "@octokit/plugin-retry": "^4.1.3",
    "@octokit/plugin-throttling": "^5.0.1",
    "@octokit/rest": "^19.0.7",
    "dotenv": "^16.0.3",
    "glob": "^9.3.2",
    "lodash": "^4.17.21",
    "pacote": "^15.1.1",
